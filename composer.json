{
    "name": "ssnepenthe/wpdb-engine-for-latitude",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "latitude/latitude": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "WpdbEngineForLatitude\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
